<!DOCTYPE html>
<html>
<head>
  <script type="text/javascript">
    function disable ()
    {
      window.history.forward();

    }

    disable();
    window.onload = disable;
    window.onpageshow = function(event)
    {
      if(event.persisted)
      {
        disable();
      }
    }

          window.onunload = function()
      {
        void(0);
      }

      //end 
  </script>
	<title></title>
</head>
<body class="Body">


      <% include include %>

      <nav class="navbar navbar-default wow fadeInDown" data-wow-duration='1s' data-wow-delay='0.5s'>
         <div class="navbar-header">
	       <a href="/" class="navbar-brand hvr-buzz-out">Quiz Maker</a>
	     </div>
      </nav>

	  <div class="container text-center">
	  	<div class="row">
			<!--The name of contest and contestant -->
      <div class="hvr-buzz-out">
			<h1 style="font-size: 30px" class="text-center wow fadeInDown" data-wow-duration='1s' data-wow-delay='1s'> Create <strong style="color: red"
      > individual</strong> and <strong style="color: green">Group</strong> Contest </h1>
      </div>
			<div class="Form">
			   <form class="form-group">
 			     <div class="col-xs-8 col-xs-offset-2 col-md-4 col-md-offset-4 wow fadeInDown" data-wow-duration='1s' data-wow-delay='1.5s'>	
			      <label class="visible-lg"> Enter The name of contest</label>
			       <input type = 'text' id = 'IDcontest' name = 'IDcontest'  placeholder=" *  Contest's Name..." 
			         class="form-control C_Name hvr-grow-shadow"> <br>
			     </div>

			     <div class="col-xs-8 col-xs-offset-2 col-md-4 col-md-offset-4 wow fadeInDown"
           data-wow-duration='1s' data-wow-delay='2s'>
			     <label class="visible-lg"> Enter Your Name</label>
			       <input type = 'text' id = 'IDcontant' name = 'IDcontant' placeholder="*  Your Name..." 
			         class="form-control F_Name hvr-grow-shadow " style="margin-bottom: 20px"> <br>
			        </div>
			  </form>
			</div>

			<div class="col-xs-6 col-xs-offset-3 col-md-2 col-md-offset-5 wow fadeInDown" data-duration='1s' data-wow-delay='2.5s'>
				       <button type="button" class="btn btn-primary btn-block" style="width: 100%" data-toggle="modal" data-target=".bs-example-modal-sm">Time</button>
                      <div class="modal fade bs-example-modal-sm" tabindex="-1" role="dialog" aria-labelledby="mySmallModalLabel">
                        <div class="modal-dialog modal-sm" role="document">
                           <div class="modal-content">
                             <ul class="list-unstyled List_Of_period">
                               <li><button id = "A07" class="btn btn-primary">1 minutes</button></li> 
                               <li><button id = "A06" class="btn btn-primary">5 minutes</button></li>
                               <li><button id = "A05" class="btn btn-primary">10 minutes</button></li>
                               <li><button id = "A04" class="btn btn-primary">15 minutes</button></li>
                    		       <li><button id = "A03" class="btn btn-primary">30 minutes</button></li>
                    		       <li><button id = "A02" class="btn btn-primary">1 hour</button></li>
                    		       <li><button id = "A01" class="btn btn-primary">2 hour</button></li>
                     		     </ul>
                           </div>
                        </div>
                      </div>
        </div>

        <!-- Time to start The contest-->
        <div class="col-xs-offset-2 col-xs-8 col-lg-offset-4 col-lg-4 wow fadeInDown  " data-wow-duration='1s' data-wow-delay='3s'>
                  <button type="button" class="btn btn-danger btn-block TimeTOStart" style="width: 100%" data-toggle="modal"  data-target=".bs-example-modal-sm4">Time To Start The Contest</button>
                     <div class="modal fade bs-example-modal-sm4" tabindex="-1" role="dialog" aria-labelledby="mySmallModalLabel">
                        <div class="modal-dialog modal-sm" role="document">
                           <div class="modal-content">
                            <div class="modal-body">
                              <div class="row">
                                <div class="col-lg-2 col-lg-offset-1">
                                 <label style="color: black;margin-left: 20px">Day</label> 
                                  <input type="number" max="30" min="0" style="color: black;" id="day">
                                </div>
                                <div class="col-lg-2 col-lg-offset-1">
                                 <label style="color: black;">Hour</label>
                                  <input type="number" max="12" min="0" style="color: black;" id="hour">
                                </div>
                                <div class="col-lg-2 col-lg-offset-1">
                                 <label style="color: black;margin-left: 2px">minute</label>
                                  <input type="number" max="24" min="0" style="color: black;" id="minute">
                                </div>

                                <div class="col-lg-12">
                                 <button class="btn btn-success" id="Button" style="margin-top: 10px" 
                                 onclick="TimeToStart()";
                                 >Click</button>
                                </div>
                              </div>
                            </div>             
                           </div>
                        </div>
                      </div>
            </div>

            <!--End Time To Start -->



			 <div class="col-xs-6 col-xs-offset-3 IndividualButton ">
			 
				  <a id = 'IndividualLINK' >
				   <button id = 'D01' class="btn btn-danger btn1 btn-block hvr-skew-forward " disabled="disabled"> Individual</button>
				  </a>

		    </div>
		     <div class="col-xs-6 col-xs-offset-3 hvr-backward">

                  <a id='link_D02' href = "/" >
				  <button id = 'D02' class="btn btn-success btn2 btn-block hvr-skew-backward " disabled="disabled"> Group </button></li>
				  </a>

		     </div>

			<!--The Type of contest-->

  <section class="loading">
    <div class="spinner">
      <div class="double-bounce1"></div>
      <div class="double-bounce2"></div> 
    </div>
  </section>

  <script type="text/javascript">   
  </script>
</body>
</html>